.profitDetails {

}
.header {
  display: flex;
  background:rgb(244, 242, 242);
  padding:10rpx 0;
  

}
.recall,.problem {
  width:50rpx;
  height:50rpx;
}
.title {
  flex:1;
  font-size:34rpx;
  font-weight:800;
  text-align: center;
}
.score {
  width:100%;
  border-bottom:4rpx solid rgb(244, 242, 242);
  padding:35rpx 15rpx;
  display: flex;
  
}
.myScore {
  font-size:32rpx;
  padding-left:10rpx;
  font-weight:600;
}
.num {
  font-size: 38rpx;
  color:rgb(234, 168, 71);
  margin-left:20rpx;
}
.scoreChange {
  width: 100%;
  display: flex;
  justify-content: space-around;
  padding:30rpx 0;
  border-bottom:4rpx solid rgb(244, 242, 242);
 
}
.change {
  font-size: 32rpx;
  font-weight: 400;

}

.on {
  color:#f00;
  font-weight: 600;
}
.time {
  width: 100%;
  padding:16rpx 30rpx;
  background:rgb(244, 242, 242);
  font-size:28rpx;
}
.from {
  
  
}
.sign {
  width:100%;
  padding:20rpx;
  border-bottom:4rpx solid rgb(244, 242, 242);
  display: flex;
  align-items:center;  
}
.left {
  flex:1;
  margin-left:20rpx;

}
.scoreFrom {
  font-size: 32rpx;
  
}
.date {
  font-size:28rpx;
  display: inline-block;
}
.right {
  font-size:48rpx;
  color:rgb(234, 168, 71);
}